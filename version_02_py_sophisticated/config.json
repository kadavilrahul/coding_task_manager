{
  "task_manager": {
    "max_concurrent_tasks": 5,
    "task_timeout": 300,
    "auto_save": true,
    "storage_backend": "json",
    "storage_path": "data/tasks.json"
  },
  "prd_generator": {
    "template_dir": "templates",
    "output_dir": "output",
    "default_format": "markdown",
    "include_diagrams": false,
    "auto_numbering": true
  },
  "analyzer_agent": {
    "analysis_depth": "deep",
    "include_metrics": true,
    "complexity_threshold": 10,
    "enable_ast_analysis": true,
    "enable_dependency_analysis": true
  },
  "planner_agent": {
    "planning_strategy": "adaptive",
    "max_iterations": 10,
    "enable_ai_suggestions": true,
    "priority_weights": {
      "high": 3,
      "medium": 2,
      "low": 1
    }
  },
  "modifier_agent": {
    "backup_enabled": true,
    "validation_enabled": true,
    "auto_format": true,
    "backup_dir": "backups",
    "max_backups": 10
  },
  "validator_agent": {
    "strict_mode": false,
    "auto_fix": true,
    "enable_syntax_check": true,
    "enable_style_check": true,
    "enable_security_check": false
  },
  "ai_utils": {
    "provider": "openai",
    "model": "gpt-3.5-turbo",
    "temperature": 0.7,
    "max_tokens": 2000,
    "timeout": 30,
    "retry_attempts": 3
  },
  "logging": {
    "level": "INFO",
    "file": "app.log",
    "max_size": "10MB",
    "backup_count": 5,
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  },
  "security": {
    "enable_input_validation": true,
    "max_file_size": "50MB",
    "allowed_file_types": [".py", ".js", ".ts", ".java", ".cpp", ".c", ".go", ".rs"],
    "sandbox_mode": false
  },
  "performance": {
    "enable_caching": true,
    "cache_ttl": 3600,
    "max_memory_usage": "1GB",
    "enable_profiling": false
  }
}